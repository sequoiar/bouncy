    },
    "devDependencies" : {
        "tap" : "0.0.x",
        "websocket-server" : "1.4.x",
        "websocket-client" : "1.0.x"
    },
    "engines" : {
        "node" : ">=0.4.0"
    },
    "license" : "MIT",
    "author" : {
        "name" : "James Halliday",
        "email" : "mail@substack.net",
        "url" : "http://substack.net"
    }
