{
    "name" : "bouncy",
    "description" : "Bounce HTTP requests around for a load balancer or http host router completely transparently",
    "version" : "0.0.4",
    "repository" : {
        "type" : "git",
        "url" : "git://github.com/substack/bouncy.git"
    },
    "main" : "index.js",
    "bin" : {
        "bouncy" : "bin/bouncy.js"
    },
    "keywords" : [
        "http",
        "parse",
        "bounce",
        "load",
        "balancer",
        "host",
        "route",
        "router",
